{
  "name": "fat-wallet",
  "version": "0.2.2",
  "private": true,
  "description": "Wallet for FAT tokens. Factom Asset Tokens (FAT) is a protocol of tokenization built on top of the Factom blockchain.",
  "author": {
    "name": "Luciap Technology Inc.",
    "email": "contact@luciap.ca"
  },
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "rm -rf dist_electron && vue-cli-service electron:build",
    "test": "mocha 'test/**/*.spec.js'",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "start": "npm run serve"
  },
  "main": "background.js",
  "dependencies": {
    "@sentry/browser": "^5.4.0",
    "@sentry/electron": "^0.17.1",
    "@sentry/integrations": "^5.4.1",
    "node-hid": "^0.7.9",
    "usb": "^1.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@fat-token/fat-js": "^1.0.1",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ledgerhq/hw-transport-node-hid": "^4.62.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "ajv": "^6.10.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "bluebird": "^3.5.5",
    "chai": "^4.2.0",
    "deepmerge": "^3.2.1",
    "electron": "^5.0.4",
    "electron-icon-builder": "^1.0.0",
    "electron-store": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "factom": "1.2.3",
    "factom-identity-lib": "^0.2.0-rc6",
    "factom-keystore": "^0.4.0",
    "fast-sha256": "^1.1.0",
    "husky": "^2.4.1",
    "jsoneditor": "^6.0.0",
    "lint-staged": "^8.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.flatmap": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mocha": "^6.1.4",
    "moment": "^2.24.0",
    "node-cache": "^4.2.0",
    "prettier": "^1.18.2",
    "roboto-fontface": "*",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tweetnacl": "^1.0.1",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-cli-plugin-electron-builder": "^1.3.4",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^1.5.14",
    "vuetify-loader": "^1.2.2",
    "vuex": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
