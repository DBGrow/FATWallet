{
  "name": "fat-wallet",
  "version": "0.1.0",
  "private": true,
  "author": "Paul Bernier",
  "description": "Wallet for FAT tokens. Factom Asset Tokens (FAT) is a protocol of tokenization built on top of the Factom blockchain.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "start": "npm run electron:serve"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@fat-token/fat-js": "0.1.0-rc6",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "ajv": "^6.10.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "bluebird": "^3.5.3",
    "deepmerge": "^3.2.0",
    "electron": "^4.0.8",
    "electron-store": "^2.0.0",
    "eslint": "^5.15.1",
    "eslint-plugin-vue": "^5.2.2",
    "factom": "1.0.0",
    "factom-identity-lib": "^0.2.0-rc6",
    "factom-keystore": "^0.2.0",
    "husky": "^1.3.1",
    "jsoneditor": "^5.30.0",
    "lint-staged": "^8.1.5",
    "lodash.debounce": "^4.0.8",
    "lodash.flatmap": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "material-design-icons-iconfont": "^4.0.5",
    "moment": "^2.24.0",
    "node-cache": "^4.2.0",
    "prettier": "^1.16.4",
    "roboto-fontface": "*",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uuid": "^3.3.2",
    "vue": "^2.6.8",
    "vue-cli-plugin-electron-builder": "^1.1.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8",
    "vuetify": "^1.5.5",
    "vuetify-loader": "^1.2.1",
    "vuex": "^3.1.0"
  },
  "main": "background.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
